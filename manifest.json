{
  "manifest_version": 2,
  "name": "Kill Tweet Stream",
  "version": "1.0.0",
  "description": "Save time by hiding the tweet stream on Twitter.",
  "homepage_url": "http://github.com/ungoldman/kill-tweet-stream",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": ["kill-tweet-stream.css"]
    }
  ]
}
